{
  "name": "leher-server",
  "version": "1.0.0",
  "description": "Backend API for Leher - Mental Health Support Platform",
  "main": "dist/index.js",
  "scripts": {
    "start": "node --max-old-space-size=4096 --expose-gc dist/index.js",
    "dev": "nodemon --max-old-space-size=4096 --expose-gc",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "seed": "node --max-old-space-size=4096 -r tsconfig-paths/register src/scripts/seed.ts",
    "seed:counselors": "node --max-old-space-size=4096 -r tsconfig-paths/register src/scripts/seedCounselorProfiles.ts",
    "migrate": "node --max-old-space-size=4096 -r tsconfig-paths/register src/scripts/migrate.ts"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@types/crypto-js": "^4.2.2",
    "@types/sentiment": "^5.0.4",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "natural": "^8.1.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "openai": "^4.20.1",
    "sentiment": "^5.0.2",
    "socket.io": "^4.7.4",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.4",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "mental-health",
    "api",
    "express",
    "mongodb",
    "leher",
    "student-support",
    "wellness"
  ],
  "author": "Leher Team",
  "license": "MIT"
}
